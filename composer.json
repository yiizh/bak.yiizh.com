{
    "name": "yiizh/yiizh.com",
    "description": "The source code of yiizh.com.",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.5",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-authclient": "*",
        "yii2tech/sitemap": "*",
        "yiizh/yii2-adminlte": "*",
        "yiizh/yii2-fontawesome": "*",
        "vlucas/phpdotenv": "*"
    },
    "require-dev": {
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-debug": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
